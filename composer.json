{
  "name": "sunnysideup/dynamic-cache-content",
  "description": "This silverstripe module allows you to cache any page and then load user specific content on top of it.",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "cache",
    "caching",
    "cached",
    "speed",
    "dynamic",
    "security",
    "seo",
    "bullet train",
    "personalised",
    "best of both worlds",
    "local storage"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\DynamicCacheContent\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
